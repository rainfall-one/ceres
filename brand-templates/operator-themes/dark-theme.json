{
  "name": "Dark Operator Theme",
  "version": "1.0.0",
  "description": "Dark mode operator branding theme",
  "colorScheme": {
    "primary": "#4fc3f7",
    "secondary": "#29b6f6",
    "accent": "#ffb74d",
    "background": "#121212",
    "surface": "#1e1e1e",
    "text": {
      "primary": "#ffffff",
      "secondary": "#b0bec5",
      "muted": "#78909c"
    },
    "status": {
      "success": "#4caf50",
      "warning": "#ff9800",
      "error": "#f44336",
      "info": "#2196f3"
    }
  },
  "typography": {
    "primary": "'Inter', -apple-system, BlinkMacSystemFont, sans-serif",
    "secondary": "'Inter', -apple-system, BlinkMacSystemFont, sans-serif",
    "monospace": "'Fira Code', 'Monaco', 'Consolas', monospace"
  },
  "spacing": {
    "baseUnit": 8,
    "scale": [0.25, 0.5, 1, 1.5, 2, 3, 4, 6, 8, 12, 16, 24]
  },
  "borderRadius": {
    "small": "4px",
    "medium": "8px",
    "large": "12px",
    "rounded": "50%"
  },
  "shadows": {
    "small": "0 1px 3px rgba(255, 255, 255, 0.12), 0 1px 2px rgba(255, 255, 255, 0.24)",
    "medium": "0 3px 6px rgba(255, 255, 255, 0.16), 0 3px 6px rgba(255, 255, 255, 0.23)",
    "large": "0 10px 20px rgba(255, 255, 255, 0.19), 0 6px 6px rgba(255, 255, 255, 0.23)"
  },
  "customization": {
    "allowLogoUpload": true,
    "allowColorCustomization": true,
    "allowTypographyCustomization": true,
    "allowCustomCSS": true,
    "requireComplianceElements": true
  }
}
