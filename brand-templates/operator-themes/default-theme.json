{
  "name": "Default Operator Theme",
  "version": "1.0.0",
  "description": "Standard operator branding theme with customizable elements",
  "colorScheme": {
    "primary": "#0066cc",
    "secondary": "#004e89",
    "accent": "#ffa400",
    "background": "#ffffff",
    "surface": "#f8f9fa",
    "text": {
      "primary": "#2c3e50",
      "secondary": "#7f8c8d",
      "muted": "#95a5a6"
    },
    "status": {
      "success": "#27ae60",
      "warning": "#f39c12",
      "error": "#e74c3c",
      "info": "#3498db"
    }
  },
  "typography": {
    "primary": "'Inter', -apple-system, BlinkMacSystemFont, sans-serif",
    "secondary": "'Inter', -apple-system, BlinkMacSystemFont, sans-serif",
    "monospace": "'Fira Code', 'Monaco', 'Consolas', monospace"
  },
  "spacing": {
    "baseUnit": 8,
    "scale": [0.25, 0.5, 1, 1.5, 2, 3, 4, 6, 8, 12, 16, 24]
  },
  "borderRadius": {
    "small": "4px",
    "medium": "8px",
    "large": "12px",
    "rounded": "50%"
  },
  "shadows": {
    "small": "0 1px 3px rgba(0, 0, 0, 0.12), 0 1px 2px rgba(0, 0, 0, 0.24)",
    "medium": "0 3px 6px rgba(0, 0, 0, 0.16), 0 3px 6px rgba(0, 0, 0, 0.23)",
    "large": "0 10px 20px rgba(0, 0, 0, 0.19), 0 6px 6px rgba(0, 0, 0, 0.23)"
  },
  "customization": {
    "allowLogoUpload": true,
    "allowColorCustomization": true,
    "allowTypographyCustomization": true,
    "allowCustomCSS": true,
    "requireComplianceElements": true
  }
}
